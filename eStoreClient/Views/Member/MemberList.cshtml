@{
}
@model List<BusinessObject.Models.Member>

<h1>Member List</h1>

<table class="table align-middle mb-0 bg-white">
	<thead class="bg-light">
		<tr>
			<th>Email</th>
			<th>Password</th>
			<th>Company</th>
			<th>City</th>
			<th>Country</th>
		</tr>
	</thead>
	<tbody>
        @foreach (var member in Model)
        {
            <tr>
                <td>@member.Email</td>
                <td>@member.Password</td>
                <td>@member.CompanyName</td>
                <td>@member.City</td>
                <td>@member.Country</td>
            </tr>
        }
	</tbody>
</table>
<a href="/Product/Index" style="background-color:gray;margin-top:10px" class="btn btn-primary">Back</a>

<a href="/Account/Logout" style="background-color:red;margin-top:10px" class="btn btn-primary">Logout</a>
